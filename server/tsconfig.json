{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 22",
  "_version": "22.14.0",
  "compilerOptions": {
    "lib": ["ES2024"],
    "module": "NodeNext",
    "target": "ES2024",
    "strict": true,
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
